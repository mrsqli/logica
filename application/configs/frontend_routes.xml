
<routes>    
                              <!-- Page Visiteur -->     
    <ProfileLogin type="Zend_Controller_Router_Route">
        <route>/Authentification</route>
        <defaults  module="frontend" controller="profile" action="login"  />
    </ProfileLogin>
    
    <UserAdd type="Zend_Controller_Router_Route">
        <route>/AjouterUser</route>
        <defaults  module="frontend" controller="user" action="add"  />
